<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<lionengine:featurable xmlns:lionengine="http://lionengine.b3dgs.com/xsd/9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://lionengine.b3dgs.com/xsd/9 http://lionengine.b3dgs.com/xsd/9/featurable.xsd">
    <lionengine:class>com.b3dgs.lionheart.object.Entity</lionengine:class>
    <lionengine:features>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Glue</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Sheet</lionengine:feature>
        <lionengine:feature>com.b3dgs.lionheart.object.feature.Turning</lionengine:feature>
    </lionengine:features>
    <lionengine:surface image="TurningAuto.png" icon="TurningAuto_ico.png"/>
    <lionengine:frames horizontal="4" vertical="4"/>
    <lionengine:size width="32" height="32"/>
    <lionengine:animations>
        <lionengine:animation name="idle" start="1" end="1" speed="0.25" reversed="false" repeat="false">
            <lionengine:collisionFramed prefix="ground" number="1" x="0" y="20" width="17" height="1"/>
        </lionengine:animation>
        <lionengine:animation name="turn" start="2" end="16" speed="0.50" reversed="false" repeat="false"/>
    </lionengine:animations>
    <lionengine:rasterable file="raster/raster1.xml" height="16" smooth="false"/>
    <lionengine:collidable group="3"/>
</lionengine:featurable>