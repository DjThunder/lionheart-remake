<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<lionengine:formulas xmlns:lionengine="http://lionengine.b3dgs.com/xsd/9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://lionengine.b3dgs.com/xsd/9 http://lionengine.b3dgs.com/xsd/9/tileFormulas.xsd">
    <lionengine:formula name="horizontal">
        <lionengine:range maxX="16" maxY="0" minX="0" minY="0" output="Y"/>
        <lionengine:function a="0.0" b="1.0" type="LINEAR"/>
    </lionengine:formula>
    <lionengine:formula name="ground">
        <lionengine:range maxX="16" maxY="0" minX="0" minY="0" output="Y"/>
        <lionengine:function a="0.0" b="0.0" type="LINEAR"/>
    </lionengine:formula>
    <lionengine:formula name="left_vertical">
        <lionengine:range maxX="0" maxY="16" minX="0" minY="0" output="X"/>
        <lionengine:function a="0.0" b="1.0" type="LINEAR"/>
    </lionengine:formula>
    <lionengine:formula name="right_vertical">
        <lionengine:range maxX="0" maxY="16" minX="0" minY="0" output="X"/>
        <lionengine:function a="0.0" b="15.0" type="LINEAR"/>
    </lionengine:formula>
</lionengine:formulas>